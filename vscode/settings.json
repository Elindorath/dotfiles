{
  "window.closeWhenEmpty": true,
  "editor.dragAndDrop": false,
  "files.insertFinalNewline": true,
  "editor.find.seedSearchStringFromSelection": true,
  "editor.scrollBeyondLastLine": true,
  "window.title": "${appName}",
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.renderLineHighlight": "all",
  "editor.cursorBlinking": "smooth",
  "editor.minimap.showSlider": "always",
  "files.defaultLanguage": "javascript",
  "files.trimFinalNewlines": true,
  "files.eol": "\n",
  "workbench.commandPalette.preserveInput": true,
  "workbench.startupEditor": "none",
  "workbench.fontAliasing": "auto",
  "workbench.statusBar.feedback.visible": false,
  "breadcrumbs.enabled": true,
  "search.smartCase": true,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shell.windows": "C\\Windows\\System32\\wsl.exe",
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "git.enableCommitSigning": true,
  "git.showPushSuccessNotification": true,
  "window.zoomLevel": 0,
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.iconTheme": "material-icon-theme",
  "git.autofetch": true,
  "redhat.telemetry.enabled": false,
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": true,
    "delay": 500,
    "exclude": [
      ".terraform/**/*",
      "**/.terraform/**/*"
    ]
  },
  "terraform.languageServer": {
    "external": true,
    "args": [
      "serve"
    ]
  },
  "files.associations": {
    "*.json": "json",
    "*.tmpl": "go-template",
    "*.yml": "ansible",
    ".envrc": "dotenv",
    ".prettierignore": "gitignore"
  },
  "explorer.confirmDragAndDrop": false,
  "polacode.backgroundColor": "#ffffff",
  "polacode.transparentBackground": true,
  "polacode.shadow": "rgba(0, 0, 0, 0.55) 0px 20px 34px",
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "yaml.customTags": [
    "!encrypted/pkcs1-oaep scalar",
    "!vault scalar"
  ],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "hadolint.hadolintPath": "/usr/local/bin/hadolint",
  "shellcheck.customArgs": [
    "-x"
  ],
  "polacode.target": "snippet",
  "go-template.languages": [
    "toml"
  ],
  "[python]": {
    "editor.tabSize": 4
  },
  "xml.server.binary.trustedHashes": [
    "c64f9b53e9983e224748f43f6be8b32af18a85f6ec88d4b0c73799c286788758"
  ],
  "cSpell.userWords": [
    "chunkname",
    "Dafuck",
    "elindorath",
    "esmodule",
    "facebook",
    "flowtype",
    "geolocation",
    "linkedin",
    "mapbox",
    "nextjs",
    "opentype",
    "shopify",
    "signin",
    "signup",
    "staycation",
    "Staycation",
    "staycations",
    "stylelint",
    "Swipeable",
    "tiktok",
    "twic",
    "unmock",
    "Westphal"
  ],
  "gitlens.rebaseEditor.ordering": "asc",
  "cSpell.language": "en,fr"
}
